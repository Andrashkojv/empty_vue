{
  "name": "vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve"    
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.13",
    "lodash": "^4.17.15",
    "vue": "^2.5.22"
  },  
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
